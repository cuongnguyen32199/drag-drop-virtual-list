{
  "env": {
      "browser": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "rules": {
      "no-case-declarations": "off",
      "react/prop-types": "off",
      "no-console": "off",
      "max-len": "off",
      "react/jsx-props-no-spreading": "off"
  },
  "parser": "babel-eslint"
}
